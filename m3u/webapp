		<script type="text/javascript">
                (function(){
                    var iMaxWidth = 980;
                    var oNavSection = $('dtv_topnav_sections_nav');
                    var oNavCustomer = $('dtv_topnav_util_nav');
                    // Co-branded sites - top nav not always present
                    if (oNavSection != undefined && oNavCustomer != undefined) {
                        var iDelta = iMaxWidth - oNavCustomer.getWidth() - oNavSection.getWidth();
                        if (Math.abs(iDelta) < 80) {
                            var aNavSectionLIs = oNavSection.down('ul').childElements();
                            var iPolarity = (iDelta > 0) ? -1 : 1;
                            var iLeftOrRight = -1;
                            for (var i=Math.abs(iDelta); i>0; i--){
                                if (i % aNavSectionLIs.length == 0){
                                    iLeftOrRight = iLeftOrRight * -1;
                                }
                                var el = aNavSectionLIs[i % aNavSectionLIs.length].down('div');
                                if (iLeftOrRight < 0){
                                    el.setStyle({paddingLeft: (parseInt(el.getStyle('paddingLeft'))-iPolarity*1)+'px'});
                                } else {
                                    el.setStyle({paddingRight: (parseInt(el.getStyle('paddingRight'))-iPolarity*1)+'px'});
                                }
                            }
                        }
                    }
                })();
            </script>
